include $(src)/Kbuild-mtk-custom-env

mali_kbase-y += \
	platform/$(MALI_PLATFORM_DIR)/mali_kbase_config_mt6833.o

ccflags-y += \
	-I$(srctree)/drivers/misc/mediatek/base/power/include
